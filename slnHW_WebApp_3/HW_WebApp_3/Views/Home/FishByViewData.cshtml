@{
    var fishs = ViewData["Fish"] as FishViewModel[];
    ViewData["Title"] = "Рыбы (ViewData)";
}

<h2>Рыбы (ViewData)</h2>
<ul>
    @foreach(var fish in fishs)
    {
        <li>@("Наименование : "+fish.Name+"; Вес : "+fish.Weight+"; Скорость : "+fish.Speed)</li>
    }
</ul>
    